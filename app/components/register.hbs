<div>
    <div>
        <h3>Already Have an Account?<LinkTo @route="login" class="button" >Login</LinkTo></h3>
    </div>
    <br>
    <h1>Register your account below</h1>
    <p class="hidden">Successfully Created Account!</p>
    <br>
    <form autocomplete="off" action="register.js" method="post">
        <label for="registerUserName">UserName: </label>
        <input type="text" id="registerUserName" />
        <br>
        <label for="registerName">Name: </label>
        <input type="text" id="registerName" />
        <br>
        <label for="registerEmail">Email: </label>
        <input type="text" id="registerEmail" />
        <br>
        <label for="registerPassword">Password: </label>
        <input type="text" id="registerPassword" />
    </form>
    <input type="submit" id="createAccountButton" class="button" value="Create Account" />
</div>

<!-- Style form -->
